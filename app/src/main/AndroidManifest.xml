<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uz.eloving.psychorotte">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.PsychoRotte">
        <activity
            android:name=".activities.ResultActivity"
            android:exported="false" />
        <activity
            android:name=".activities.QuizActivity"
            android:exported="false"
            android:theme="@style/Theme.PsychoRotteLoginActivity" />
        <activity
            android:name=".activities.SetUsernameActivity"
            android:exported="false"
            android:theme="@style/Theme.PsychoRotteLoginActivity" />
        <activity
            android:name=".activities.SetPasswordActivity"
            android:exported="false"
            android:theme="@style/Theme.PsychoRotteLoginActivity" />
        <activity
            android:name=".activities.CodeConfirmationActivity"
            android:exported="false"
            android:theme="@style/Theme.PsychoRotteLoginActivity" />
        <activity
            android:name=".activities.LoginActivity"
            android:exported="false"
            android:theme="@style/Theme.PsychoRotteLoginActivity" />
        <activity
            android:name=".activities.MainActivity"
            android:exported="false"
            android:label="@string/title_activity_main" />
        <activity
            android:name=".activities.HandleActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>